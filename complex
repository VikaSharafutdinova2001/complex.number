#include <iostream>

class Complex
{
private:
 double Re, Im;

public:
    Complex();
    Complex(double x);
    Complex(double x, double y);
    double re() {return Re;}
    double im() {return Im;}
};

int main()
{
  Complex z; 
 } 
 
 Complex::Complex(){
    Re=0;
    Im=0;
 }
    
 Complex::Complex(double x){
    Re=x;
    Im=0;
 }
    
Complex::Complex(double x, double y){
    Re=x;
    Im=y;
}
